/* 	ISDIGIT

#include "libft.h"
int main(void)
{
    printf("%d\n",ft_isdigit("1"));
}
 */


/* 	ISALNUM


#include "libft.h"

int main(void)
{
    printf("%d\n", ft_isalnum('('));
    printf("%d\n", ft_isalnum('1'));
    printf("%d\n", ft_isalnum('a'));
} */


/* 
	ISASCII

#include "libft.h"

int main(void)
{  
    printf("%d\n", ft_isascii(L'€')); //L prefix is for wide character like '€'
    printf("%d\n", ft_isascii('a')); 
}
 */

/*     ISPRINT

#include "libft.h"
